{
  "name": "Path",
  "version": "0.1.0",
  "description": "Flat Path Editor",
  "author": "Braden Steffaniak",
  "license": "ISC",
  "defaultTarget": "es6",
  "scripts": {
    "test": "node dist/es6/test.js",
    "cmd": "node dist/index.js"
  },
  "sources": {
    "main": {
      "source": "src",
      "dependencies": {
        "https://github.com/FlatLang/System": "master",
        "https://github.com/FlatLang/IO": "master",
        "https://github.com/FlatLang/Logger": "master",
        "https://github.com/FlatLang/RegEdit": "master"
      }
    },
    "cmd": {
      "mainClass": "flat/path/Path_Command",
      "source": "src",
      "dependencies": {}
    },
    "test": {
      "mainClass": "flat/path/Path_TestSuite",
      "source": "src/**/*_Test*.flat",
      "dependencies": {
        "https://github.com/FlatLang/Test": "master"
      }
    }
  }
}
